<!-- Contact Content-->

<!-- Form -->
<form method = "POST" action = "mailto:john.shin53@gmail.com">
    <!-- outline box -->
    <fieldset id = "field">
    <!-- title -->
    <legend>INFO FORM</legend>

    <!-- table -->
    <table>
        <tr>
            <td><label for="f_name">First name:</label></td>
            <td class = "inputdata"><input type = "text" name = "fname" id = "f_name" size = "30"></td>
        </tr>
        <tr>
             <td><label for="l_name">Last name:</label></td>
            <td class = "inputdata"><input type = "text" name = "lname" id = "l_name" size = "30"></td>           
        </tr>
        <tr>
            <td><label for="phone">Contact Number:</label></td>
            <td class = "inputdata"><input type="tel" id="phone" name="phone" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}"></td>  
        </tr>
        <tr>
            <td><label for="email">Email:</label></td>
            <td class = "inputdata"><input type="email" id="email" name="email"></td>  
        </tr>
        <tr>
            <td><label for="message">Message:</label></td>
            <td class = "inputdata"><textarea  id = "message" class = "message" cols = 33></textarea></td>
        </tr>
        </td>
        <!-- buttons-->
        <tr>
            <td>
                <button id = "cancel" type="button" class="btn btn-dark" value = "start again">CANCEL</button>
                <button type="submit" id = "submitbutton" class="btn btn-dark" value = "register now">SUBMIT</button>
            </td>
        </tr>
    </table>
</fieldset>